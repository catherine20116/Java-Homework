import java.util.Arrays;

public class uTask21 {

	public static void main(String[] args) {
		// Uzrakstīt programmu, kura izveido divus Integer sarakstus ar patvaļīgām vērtībām. Apvieno
		// sarakstus vienā jaunā sarakstā, kāpinot visus elementus 3. pakāpē.
		// которая создает 2 списка integeres со свободными значениями. Соединить списки в один список , возведя все елементы в 3. степень 
		int[] firstArray = {1,5,6,4};        //source array  
		int[] secondArray = {2,3};  //destination array  
		int fal = firstArray.length;        //determines length of firstArray  
		int sal = secondArray.length;   //determines length of secondArray  
		int[] result = new int[fal + sal];  //resultant array of size first array and second array  
		System.arraycopy(firstArray, 0, result, 0, fal);  
		System.arraycopy(secondArray, 0, result, fal, sal);  
		for (int i = 0; i < result.length; i++) {
			result[i] = result[i] * result[i] * result[i];
		}
		System.out.println(Arrays.toString(result));    //prints the resultant array  
	}

}
