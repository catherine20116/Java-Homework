import java.util.Arrays;

public class uTask28 {
	
	public static Object[] reverseArray (Object[] genericVal) {
		for (int i = 0; i < genericVal.length / 2; i++) {
			Object temp = genericVal[i];
			genericVal[i] = genericVal[genericVal.length - 1];
			genericVal[genericVal.length - 1] = temp;
		}
		return genericVal;
	}

	public static void main(String[] args) {
		// Uzrakstīt metodi, reverseArray, kura parametrā saņem masīvu un rezultātā atgriež masīvu
		// apgrieztu otrādāk. reverseArray jāstrādā gan ar String, gan int masīviem.
		
		int[] array = {1, 3, 5, 2};
		String[] array1 = {"AB","CDE","FEG"};		
		System.out.println(Arrays.toString(reverseArray(array)));
	}

}
